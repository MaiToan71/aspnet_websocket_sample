<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <GenerateFullPaths>true</GenerateFullPaths>
    <RuntimeFrameworkVersion>2.1.0</RuntimeFrameworkVersion>
    <RuntimeIdentifier>win-x64</RuntimeIdentifier>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <PlatformTarget>AnyCPU</PlatformTarget>
  </PropertyGroup>

  <PropertyGroup>
    <RestoreSources>$(RestoreSources);../nuget_packages;</RestoreSources>
  </PropertyGroup> 
    
  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore" Version="2.1.1-rtm-t000" />
    <PackageReference Include="Microsoft.AspNetCore.Http" Version="2.1.1-rtm-t000" />
    <PackageReference Include="Microsoft.AspNetCore.HttpsPolicy" Version="2.1.1-rtm-t000" />
    <PackageReference Include="Microsoft.AspNetCore.Server.Kestrel" Version="2.1.1-rtm-t000" />
    <PackageReference Include="Microsoft.AspNetCore.Server.Kestrel.Core" Version="2.1.1-rtm-t000" />
    <PackageReference Include="Microsoft.AspNetCore.StaticFiles" Version="2.1.1-rtm-t000" />
    <PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.1.1-rtm-t000" />
    <PackageReference Include="Microsoft.AspNetCore.Http.Extensions" Version="2.1.1-rtm-t000" />
    <PackageReference Include="Microsoft.AspNetCore.Http.Features" Version="2.1.1-rtm-t000" />
    <PackageReference Include="Microsoft.AspNetCore.WebSockets" Version="2.1.1-rtm-t000" />
    <PackageReference Include="Microsoft.AspNetCore.WebUtilities" Version="2.1.1-rtm-t000" />
    <PackageReference Include="Microsoft.Extensions.Options" Version="2.1.1-rtm-t000" />
    <PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="2.1.1-rtm-t000" />
    <PackageReference Include="Microsoft.Net.Http.Headers" Version="2.1.1-rtm-t000" />
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="Serilog.AspNetCore" Version="2.1.1" />
    <PackageReference Include="Serilog.Sinks.Console" Version="3.1.1" />
    <PackageReference Include="Serilog.Sinks.Seq" Version="4.0.0" />
    <PackageReference Include="Serilog.Settings.Configuration" Version="2.6.1" />
    <PackageReference Include="Serilog.Sinks.Trace" Version="2.1.0" />
    <PackageReference Include="System.Text.Encoding" Version="4.3.0" />
    <PackageReference Include="System.Text.Encoding.CodePages" Version="4.5.0" />
    <PackageReference Include="System.Text.Encoding.Extensions" Version="4.3.0" />
  </ItemGroup>
</Project>